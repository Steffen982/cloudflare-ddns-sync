{
  "name": "cloudflare-ddns-sync",
  "version": "2.0.0",
  "description": "A simple module to update DNS records on Cloudflare whenever you want",
  "main": "dist/index.js",
  "author": "Steffen Knaup <SteffenKnaup@hotmail.de>",
  "license": "ISC",
  "keywords": [
    "cloudflare",
    "cddnss",
    "ddns",
    "dns",
    "dyndns",
    "sync"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com:Steffen982/cloudflare-ddns-sync.git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "test": "c8 mocha dist/tests/*.js --timeout 15000 --exit",
    "test-jenkins": "JUNIT_REPORT_PATH=report.xml npm test -- --reporter mocha-jenkins-reporter",
    "test-coverage-check": "c8 check-coverage --lines 70 --functions 70 --branches 70",
    "test-coverage-report": "c8 report"
  },
  "dependencies": {
    "cloudflare": "2.7.0",
    "node-cron": "2.0.3",
    "parse-domain": "2.3.2",
    "public-ip": "3.2.0",
    "what-is-my-ip-address": "1.0.3"
  },
  "devDependencies": {
    "@types/chai": "4.2.3",
    "@types/minimist": "1.2.0",
    "@types/mocha": "5.2.7",
    "@types/node": "10.14.17",
    "@types/node-cron": "2.0.2",
    "c8": "5.0.4",
    "chai": "4.2.0",
    "minimist": "1.2.0",
    "mocha": "6.2.0",
    "mocha-jenkins-reporter": "0.4.2",
    "tslint": "5.20.0",
    "typescript": "3.6.3"
  }
}
